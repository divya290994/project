<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>applicationcontextconfig</servlet-name>
  	<display-name>applicationcontextconfig</display-name>
  	<description></description>
  	<servlet-class>com.niit.shopbackend.config.applicationcontextconfig</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>categoryDAO</servlet-name>
  	<display-name>categoryDAO</display-name>
  	<description></description>
  	<servlet-class>com.niit.shopbackend.DAO.categoryDAO</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>category</servlet-name>
  	<display-name>category</display-name>
  	<description></description>
  	<servlet-class>com.niit.shopbackend.model.category</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>applicationcontextconfig</servlet-name>
  	<url-pattern>/applicationcontextconfig</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>categoryDAO</servlet-name>
  	<url-pattern>/categoryDAO</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>category</servlet-name>
  	<url-pattern>/category</url-pattern>
  </servlet-mapping>
</web-app>
